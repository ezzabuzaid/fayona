{
  "scripts": {
    "snyk": "snyk test",
    "test": "clear && jest --detectOpenHandles --passWithNoTests --silent --verbose",
    "test:watch": "clear && npm test --watch --runInBand",
    "dev": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "watch:dev": "clear && nodemon -e ts -w ./src -x npm run dev",
    "watch:prod": "clear && node -r ./tsconfig-paths-bootstrap.js ./dist/main.js",
    "start": "npm run watch:prod",
