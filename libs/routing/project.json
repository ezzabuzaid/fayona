{
  "root": "libs/routing",
  "sourceRoot": "libs/routing/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/routing",
        "main": "libs/routing/src/index.ts",
        "tsConfig": "libs/routing/tsconfig.lib.json",
        "assets": ["libs/routing/*.md"]
      }
    },
    "publish": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "noBuild": true,
        "access": "public",
        "buildTarget": "production"
      }
    },
    "beta-publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "noBuild": false,
        "access": "public",
        "buildTarget": "production",
        "tag": "beta"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/routing/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/routing"],
      "options": {
        "jestConfig": "libs/routing/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
