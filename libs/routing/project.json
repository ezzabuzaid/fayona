{
  "root": "libs/routing",
  "sourceRoot": "libs/routing/src",
  "targets": {
    "build": {
      "executor": "./tools/executors/pages:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/routing",
        "main": "libs/routing/src/index.ts",
        "tsConfig": "libs/routing/tsconfig.lib.json",
        "assets": ["libs/routing/*.md"]
      }
    },
    "release": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "",
        "baseBranch": "master",
        "push": true,
        "postTargets": ["routing:github", "routing:publish"]
      }
    },
    "pre-release": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "",
        "baseBranch": "beta",
        "releaseAs": "prerelease",
        "preid": "beta",
        "push": true,
        "postTargets": ["routing:github", "routing:beta-publish"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "noBuild": false,
        "access": "public",
        "buildTarget": "production"
      }
    },
    "beta-publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "noBuild": false,
        "access": "public",
        "buildTarget": "production",
        "tag": "beta"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/routing/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/routing"],
      "options": {
        "jestConfig": "libs/routing/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
