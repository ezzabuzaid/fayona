{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": [
      "es2015"
    ],
    "declaration": false,
    "outDir": "./dist/",
    "removeComments": true,
    "downlevelIteration": true,
    "strict": false,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "resolveJsonModule": true,
    "paths": {
      "@core/*": [
        "app/core/*"
      ],
      "@api/*": [
        "app/api/*"
      ],
      "@shared/*": [
        "app/shared/*"
      ],
      "@environment/*": [
        "environment/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@test/*": [
        "test/*"
      ]
    },
    "pretty": true,
    "diagnostics": true,
    "noStrictGenericChecks": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "jest"
    ],
  },
  "typedocOptions": {
    "mode": "modules",
    "out": "docs",
    "exclude": [
      "**/*+(index|.spec|.e2e).ts",
      "./src/test/**/*",
      "./global.d.ts"
    ],
    "excludePrivate":true,
    "excludeProtected":true
  }
}